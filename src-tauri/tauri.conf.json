{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "tba",
  "version": "0.5.7",
  "identifier": "com.flespi.tba.dev",
  "build": {
    "frontendDist": "../dist/spa",
    "devUrl": "http://localhost:9000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "tba",
        "width": 700,
        "height": 500,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
